#!/usr/bin/python3
import math
import sys
# 931704858942888746196869


def gcd(x, y):
    if y == 0:
        return x
    return gcd(y, x % y)

def prime_sqrt(n):
    for i in range(2,round(math.sqrt(n))+1):
        if n % i == 0:
            return i
    return 1

if len(sys.argv) != 2:
    sys.exit(1)
try:
    f = open(sys.argv[1], "r")
except e:
    sys.exit(1)


for line in f:
    number = int(line)
    p = prime_sqrt(number)
    q = number // p
    if p > q:
        p, q = q, p
    print(f"{number}={q}*{(p)}")
f.close()
