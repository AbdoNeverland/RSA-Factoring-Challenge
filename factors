#!/usr/bin/python3
import math
import sys
# 931704858942888746196869

def fact(number):
    if number % 2 == 0:
        return 2
    for k in range(3, int(math.sqrt(number)), 2):
        if number % k == 0:
            return k
    return 1


if len(sys.argv) != 2:
    sys.exit(1)
try:
    f = open(sys.argv[1], "r")
except e:
    sys.exit(1)


for line in f:
    number = int(line)
    p = fact(number)
    q = number // p
    if p > q:
        p, q = q, p
    print(f"{number}={q}*{(p)}")
f.close()
