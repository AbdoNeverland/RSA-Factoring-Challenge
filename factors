#!/usr/bin/python3
import sys
from random import randrange

c = 1


def gcd(x, y):
    if y == 0:
        return x
    return gcd(y, x % y)


def g(x, c):
    return ((x * x + c)%n)


def getf(n):
    global c
    if (n == 1):
        return n
    for k in range(2,10):
        if n % k == 0:
            return k
    x = y = 2
    c += 1
    # print(f"c = {c}")
    d = 1

    while d == 1:
        x = g(x, c)
        y = g(g(y, c), c)
        y = g(g(y, c), c)
        d = gcd(abs(x - y), n)


    if d == n:
        return getf(n)
    else:
        return d


def is_prime_smaller(n):
    for i in range(2, n // 2 + 1):
        if n % i == 0:
            return False
    return True


def is_prime(n, prec=1):
    if n < 10:
        return is_prime_smaller(n)
    else:
        for i in range(prec):
            a = randrange(2, n - 2 + 1)
            if pow(a, n - 1, n) != 1:
                return False
    return True

if len(sys.argv) != 2:
    sys.exit(1)
try:
    f = open(sys.argv[1], "r")
except:
    sys.exit(1)
for line in f:
    n = int(line)
    p = 1
    if is_prime(n):
        p = 1
    else:
        p = getf(n)
    q = n // p
    if p > q:
        p,q=q,p
    print(f"{n}={q}*{(p)}")
f.close()
